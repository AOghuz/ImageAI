{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Services": {
    "WalletApiUrl": "https://localhost:5246"
  },

  "Jwt": {
    "Issuer": "imageaiproject.identity",
    "Audience": "imageaiproject.clients",
    "Secret": "a-string-secret-at-least-256-bits-long"
  },

  "Fal": {
    "Key": "", // GÜVENLİK: Burası boş bırakıldı. Program.cs Environment'tan okuyacak.
    "PollIntervalSeconds": 2,
    "PollTimeoutSeconds": 180
  },

  "FalModels": {
    // --- FLUX ---
    "fal-ai/flux-kontext/dev": {
      "ModelPath": "fal-ai/flux-kontext/dev",
      "DefaultOutputFormat": "jpeg"
    },
    "fal-ai/flux-kontext-lora": {
      "ModelPath": "fal-ai/flux-kontext-lora/text-to-image",
      "DefaultOutputFormat": "jpeg"
    },
    "fal-ai/flux/srpo": {
      "ModelPath": "fal-ai/flux/srpo",
      "DefaultOutputFormat": "jpeg"
    },

    // --- IMAGEN ---
    "fal-ai/imagen4/preview/fast": {
      "ModelPath": "fal-ai/imagen4/preview/fast",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/imagen4/preview/ultra": {
      "ModelPath": "fal-ai/imagen4/preview/ultra",
      "DefaultOutputFormat": "png"
    },

    // --- NANO & QWEN ---
    "fal-ai/nano-banana": {
      "ModelPath": "fal-ai/nano-banana",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/nano-banana/edit": {
      "ModelPath": "fal-ai/nano-banana/edit",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/qwen-image": {
      "ModelPath": "fal-ai/qwen-image",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/qwen-image-edit": {
      "ModelPath": "fal-ai/qwen-image-edit",
      "DefaultOutputFormat": "png"
    },

    // --- IDEOGRAM ---
    "fal-ai/ideogram/v3": {
      "ModelPath": "fal-ai/ideogram/v3",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/ideogram/v3/replace-background": {
      "ModelPath": "fal-ai/ideogram/v3/replace-background",
      "DefaultOutputFormat": "png"
    },

    // --- UPSCALE & UTILS ---
    "fal-ai/topaz/upscale/image": {
      "ModelPath": "fal-ai/topaz/upscale/image",
      "DefaultOutputFormat": "jpeg"
    },
    "fal-ai/aura-sr": {
      "ModelPath": "fal-ai/aura-sr",
      "DefaultOutputFormat": "png"
    },
    "fal-ai/bria/background/remove": {
      "ModelPath": "fal-ai/bria/background/remove",
      "DefaultOutputFormat": "png"
    }
  },

  "Storage": {
    "AutoCleanupMinutes": 60
  }
}