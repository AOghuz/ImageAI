{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "imageaiproject.identity",
    "Audience": "imageaiproject.clients",
    "Secret": "a-string-secret-at-least-256-bits-long"
  },
  "Services": {
    "WalletApi": "https://localhost:7269"
  },
  "Pricing": {
    "BackgroundRemovalV1": 75,
    "BackgroundRemovalV2": 50,
    "BackgroundRemovalV3": 100,
    "Fal": {
      "nano-banana": {
        "TextToImage": 60,
        "ImageEdit": 80
      },
      "flux-kontext-dev": {
        "ImageEdit": 100
      },
      "qwen-image-edit": {
        "ImageEdit": 90
      },
      "seedream-v4-edit": {
        "ImageEdit": 80
      },
      "flux-kontext-lora": {
        "TextToImage": 120,
        "ImageEdit": 130,
        "Inpaint": 140
      },
      "qwen-image": {
        "TextToImage": 80
      },
      "seedream": {
        "TextToImage": 100
      },
      "seedream-v3": {
        "TextToImage": 110,
        "ImageEdit": 120
      },
      "flux-srpo": {
        "ImageEdit": 140
      },
      "imagen-4": {
        "TextToImage": 150,
        "ImageEdit": 160
      },
      "imagen-4-ultra": {
        "TextToImage": 180,
        "ImageEdit": 200
      },
      "bg-remove": {
        "ImageEdit": 50
      },
      "ideogram-v3-replace-background": {
        "ImageEdit": 90
      },
      "aura-sr": {
        "Upscale": 120
      },
      "topaz-upscale": {
        "Upscale": 150
      }
    }
  },
  "Fal": {
    "BaseUrl": "https://queue.fal.run",
    "PollIntervalSeconds": 2,
    "PollTimeoutSeconds": 120
  },
  "FalModels": {
    "nano-banana": {
      "ModelPath": "fal-ai/nano-banana",
      "SupportsTextToImage": true,
      "SupportsImageEdit": true,
      "MaxImages": 4,
      "DefaultOutputFormat": "jpeg"
    },
    "flux-kontext-dev": {
      "ModelPath": "fal-ai/flux-kontext",
      "SupportsTextToImage": false,
      "SupportsImageEdit": true,
      "MaxImages": 4,
      "DefaultOutputFormat": "jpeg"
    },
    "qwen-image-edit": {
      "ModelPath": "fal-ai/qwen-image-edit",
      "SupportsTextToImage": false,
      "SupportsImageEdit": true,
      "MaxImages": 4,
      "DefaultOutputFormat": "png"
    },
    "seedream-v4-edit": {
      "ModelPath": "fal-ai/bytedance/seedream/v4/edit",
      "SupportsTextToImage": false,
      "SupportsImageEdit": true,
